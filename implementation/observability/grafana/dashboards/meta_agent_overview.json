{
  "title": "Meta-Agent Overview",
  "timezone": "browser",
  "editable": true,
  "style": "dark",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Meta-Agent Latency (p50/p95)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(neuraforge_meta_agent_resolution_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50 latency",
          "interval": ""
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(neuraforge_meta_agent_resolution_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 latency",
          "interval": ""
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "Dispute Rate",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "rate(neuraforge_meta_agent_disputes_total[5m])",
          "legendFormat": "disputes/min",
          "interval": ""
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "type": "stat",
      "title": "Escalation Rate (rolling 5m)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(increase(neuraforge_orchestrator_escalations_total[5m]))",
          "interval": ""
        }
      ],
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8}
    },
    {
      "type": "stat",
      "title": "Active Review Tickets",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "neuraforge_review_tickets_open",
          "interval": ""
        }
      ],
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8}
    },
    {
      "type": "timeseries",
      "title": "Resolution Confidence",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "avg_over_time(neuraforge_meta_agent_confidence[15m])",
          "legendFormat": "avg confidence",
          "interval": ""
        }
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12}
    }
  ]
}